{
    "name": "brenoroosevelt/psr-nullcontainer",
    "description": "Null Object Pattner for PSR-11 Container",
    "keywords": [
        "psr-11",
        "null object pattern",
        "dummy container"
    ],
    "homepage": "https://github.com/brenoroosevelt/psr-nullcontainer",
    "license": "MIT",
    "authors": [
        {
            "name": "Breno Roosevelt",
            "email": "breno.roosevelt@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "psr/container": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3",
        "phpstan/phpstan": "^1"
    },
    "autoload": {
        "psr-4": {
            "BrenoRoosevelt\\Psr11\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrenoRoosevelt\\Psr11\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check": [
            "phpunit tests",
            "phpcs -p src tests",
            "phpstan analyse src"
        ],
        "test": [
            "phpunit tests"
        ],
        "cs-fix": [
            "phpcbf src"
        ]
    }
}